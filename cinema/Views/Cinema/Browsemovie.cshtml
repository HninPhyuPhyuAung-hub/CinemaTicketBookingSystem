        @using PagedList.Mvc;


        <main id="content" role="main">
            <!-- End News Carousel -->


            <br />


            <div class="row">

                <br /><br />
                <br /><br /><br />
                @foreach (var item in Model.movieList)
                {
                    <div class="col-md-3" id="card-@item.movieid">
                        <div class="custom-card">
                            <div class="text-center" style="padding-top:15px">
                                <img src="@item.moviephotho" width="320" height="240" />
                                  
                            </div>
                            @if (item.moviename == null)
                            {
                                <h4 class="text-center myanmar3font" style="padding-bottom:7px;">Unknown </h4>
                            }
                            else
                            {
                                <h4 class="text-center myanmar3font" style="padding-bottom:7px;">@item.moviename </h4>
                            }
                            @if (item.showingevent == null)
                            {
                                <h4 class="text-center myanmar3font" style="padding-bottom:7px;">Unknown </h4>
                            }
                            else
                            {
                                <h4 class="text-center myanmar3font" style="padding-bottom:7px;">@item.showingevent </h4>
                            }
                            <div class="row" style="text-align:center;padding-left:90px">
                                <a href="@Url.Action("ViewDetail", "Cinema",new { movieid = @item.movieid })" class="btn btn-sm btn-primary" style="padding-left:17px;padding-right:17px;margin-right:5px">View Detail</a> 
                                <a href="@Url.Action("BookMovie", "Booking",new { movieid = @item.movieid })" class="btn btn-sm btn-primary" style="padding-left:17px;padding-right:17px;">Book Now</a>
                               
                                <br />
                            </div>
                        </div>
                    </div>

                }





