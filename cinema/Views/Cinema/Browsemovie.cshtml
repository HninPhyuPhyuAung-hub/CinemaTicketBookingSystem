        function showlist(action, div) {
            $.ajax({
                cache: false,
                url: action,
                beforeSend: function () {
                    $(div).empty().append('<div class="text-center"><div class="lds-ellipsis"><div></div><div></div><div></div><div></div></div></div>');
                },
                success: function (myData) {
                    $(div).empty().append(myData);
                },
                complete: function () {
                }
            });
        }
    </script>

